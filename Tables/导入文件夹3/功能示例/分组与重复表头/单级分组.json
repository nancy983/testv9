  "BuiltInViewInfo": {
    "ViewName": "单级分组",
    "CreateViewSql": "SELECT\r\n    产品.*,\r\n    类别.类别名称,\r\n    类别.说明 AS 类别说明,\r\n    供应商.公司名称 AS 供应商,\r\n    供应商.联系人姓名,\r\n    供应商.城市,\r\n    供应商.地址\r\nFROM\r\n    (\r\n        供应商\r\n        INNER JOIN 产品 ON 供应商.供应商ID = 产品.供应商ID\r\n    )\r\n    INNER JOIN 类别 ON 产品.类别ID = 类别.类别ID\r\nORDER BY\r\n    产品.类别ID;"
  },
